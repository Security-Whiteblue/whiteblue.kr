	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<meta name="author" content="dev-ys-36">
	<meta name = "keywords" content="Security Whiteblue">
	<meta name="description" content="Security Whiteblue">
	<meta property="og:title" content="Security Whiteblue">
	<meta property="og:url" content="">
	<meta property="og:description" content="Security Whiteblue">
	<!-- bootstrap -->
	<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
	<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
	<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.8.1/font/bootstrap-icons.css">
	<!-- summernote -->
	<script src="https://code.jquery.com/jquery-3.4.1.slim.min.js" integrity="sha384-J6qa4849blE2+poT4WnyKhv5vZF5SrPo0iEjwBvKU7imGFAV0wwj1yYfoRSJoZ+n" crossorigin="anonymous"></script>
	<link href="https://cdn.jsdelivr.net/npm/summernote@0.8.18/dist/summernote-lite.min.css" rel="stylesheet">
	<script src="https://cdn.jsdelivr.net/npm/summernote@0.8.18/dist/summernote-lite.min.js"></script>
	<!-- stylesheet css -->
	<link rel="stylesheet" href="https://whiteblue.kr/css/main.css">
</head>
<body data-spy="scroll" data-target=".navbar-collapse">

	<%-include('tags/alert')%>

	<%-include('tags/nav')%>

	<%-include('tags/header')%>

	<!-- section -->
	<section class="container">
		<div class="row">
			<div class="notice">
				<form name="write_form" action="/write" method="post">
					<span style="color: white;">제목: </span><input type="text" id="subject" name="subject">
					<br>
					<br>
					<textarea id="summernote" name="editordata"></textarea>
					<input type="submit" value="제출">
				</form>
			</div>
		</div>
	</section>

	<script>
	$(document).ready(function(){
		var fontList = ['NanumSquare'];
		$('#summernote').summernote({
			fontNames: fontList,
			fontNamesIgnoreCheck: fontList,
			placeholder: '내용을 입력하세요..',
			height: 140
		});
	});
	</script>

	<%-include('tags/footer')%>

</body>
</html>